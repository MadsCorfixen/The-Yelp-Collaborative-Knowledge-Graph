<https://purl.archive.org/purl/yckg/vocabulary>
  a owl:Ontology ;
  dc:title "The Yelp ontology"@en ;
  dc:description "The Yelp ontology provides context to concepts and entities found in the Yelp Open Dataset"@en ;
  ns0:preferredNamespacePrefix "yelpvoc" ;
  ns0:preferredNamespaceUri "https://purl.archive.org/purl/yckg/vocabulary#" ;
